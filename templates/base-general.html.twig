<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>BK import - export</title>
	<!-- Bootstrap -->
	<link href="{{asset('css/bootstrap-4.3.1.css')}}" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="{{asset('css/jquery.fancybox.min.css')}}">
	<link href="{{asset('css/style.css')}}" rel="stylesheet">

	<link href="https://fonts.googleapis.com/css2?family=Patua+One&display=swap" rel="stylesheet">
</head>

<body>
	<nav class="navbar fixed-top navbar-expand-xl navbar-dark redBg">
		<a class="navbar-brand" href="#"><img src="{{asset('images/logo.png')}}" /></a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
			aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<div class="collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
			<ul class="navbar-nav ml-auto text-center" id="menu">
				<li id="homeLink" class="who-active nav-item active">
					<a class="nav-link text-uppercase" href="{{ path('home') }}">{% trans %}h_inicio{% endtrans %}<span
							class="sr-only">(current)</span></a>
				</li>
				<li id="tradeLink" class="who-active nav-item">
					<a class="nav-link text-uppercase" href="{{ path('mercadotecnia') }}">{% trans %}h_mercadotecnia{% endtrans %}</a>
				</li>

				<li id="impLink" class="who-active nav-item">
					<a class="nav-link text-uppercase" href="{{ path('importaciones') }}">{% trans %}h_importaciones{% endtrans %}</a>
				</li>

				<li id="expLink" class="who-active nav-item">
					<a class="nav-link text-uppercase" href="{{ path('exportaciones') }}">{% trans %}h_exportaciones{% endtrans %}</a>
				</li>

				<li id="logLink" class="who-active nav-item">
					<a class="nav-link text-uppercase" href="{{ path('logistica') }}">{% trans %}h_logistica{% endtrans %}</a>
				</li>
				<li id="contactLink" class="who-active nav-item">
					<a class="nav-link text-uppercase" href="{{ path('contacto') }}">{% trans %}h_contacto{% endtrans %}</a>
				</li>
				<li class="dropdown-divider"></li>
			</ul>
			<ul class="navbar-nav text-center" id="usuario">
				{% if app.user %}
				<li>{{ app.user.nombreApellidos }}
					<a href="{{ path('app_logout') }}">{% trans %}b_cerrar_sesion{% endtrans %}</a>
				</li>
				{% else %}
				<li>
					<a href="{{ path('app_login') }}">{% trans %}b_iniciar_sesion{% endtrans %}</a>
				</li>
				{% endif %}
				<li class="dropdown-divider"></li>
			</ul>

			<ul class="navbar-nav text-center" id="idiomas">
				<li><a href="/es"><img src="{{asset('images/esp.png')}}" alt="spanish" /></a><a href="/en"><img
							src="{{asset('images/eng.png')}}" alt="english" /></a></li>
			</ul>
		</div>
	</nav>

	{% block body %}
	{% endblock %}

	<footer class="text-lg-center">
		<div class="fluid-container">
			<div class="container myContainer">
				<div class="row">
					<div class="col-md-12 col-sm-12 col-lg-4 text-center text-lg-left ">
						<h2>{% trans %}h_sitios_interes{% endtrans %}</h2>
						<ul>
							<li><a target="_blank" href="https://www.mindus.gob.cu/es">{% trans %}link_industria{% endtrans %}</a></li>
							<li><a target="_blank" href="https://www.mincex.gob.cu">{% trans %}link_comercio{% endtrans %}</a></li>
							<li><a target="_blank" href="http://www.minrex.gob.cu/es">{% trans %}link_exterior{% endtrans %}</a></li>
							<li><a target="_blank" href="http://www.zedmariel.com/es">{% trans %}link_mariel{% endtrans %}</a></li>
							<li><a target="_blank" href="http://www.procuba.cu/es">{% trans %}link_promocion{% endtrans %}</a></li>
							<li><a target="_blank" href="http://misiones.minrex.gob.cu/es/camara-de-comercio">{% trans %}link_camara{% endtrans %}</a></li>
							<li><a target="_blank" href="https://www.sime.cu/es">GESIME</a></li>
							<li><a target="_blank" href="https://www.aduana.gob.cu/">{% trans %}link_aduana{% endtrans %}</a></li>
						</ul>




					</div>
					<div class="col-md-12 col-sm-12 col-lg-4 text-center" id="redes-footer">
						<h2>{% trans %}h_siguenos{% endtrans %}</h2>
						<a href="#"><img src="{{asset('images/btn-facebook.png')}}" /></a>
						<a href="#"><img src="{{asset('images/btn-twitter.png')}}" /></a>
						<p>{% trans %}p_siguenos_redes{% endtrans %}</p>

					</div>
					<div class="col-md-12 col-sm-12 col-lg-4 text-center text-lg-right">
						<h2>{% trans %}h_contactanos{% endtrans %}</h2>
						<form id="formContactFooter">
							<div class="form-group">
								<input type="text" class="form-control" id="inputNombreFooter" placeholder="{% trans %}tx_nombre{% endtrans %}">
							</div>
							<div class="form-group">
								<input type="email" class="form-control" id="inputEmailFooter" placeholder="{% trans %}tx_email{% endtrans %}">
							</div>
							<div class="form-group">
								<textarea></textarea>
							</div>
							<div class="form-group">
								<button type="submit" class="btn btn-primary">{% trans %}b_enviar{% endtrans %}</button>
							</div>
						</form>
					</div>
				</div>

			</div>
		</div>
		<div class="row">
			<div class="col-12 text-center">
				<p>Copyright Â© {{ "now"|date("Y") }}</p>
			</div>
		</div>
	</footer>

	<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
	<script src="{{asset('js/jquery-3.3.1.min.js')}}"></script>
	<!-- Include all compiled plugins (below), or include individual files as needed -->
	<script src="{{asset('js/popper.min.js')}}"></script>
	<script src="{{asset('js/bootstrap-4.3.1.js')}}"></script>
	<script src="{{asset('js/jquery.fancybox.min.js')}}"></script>
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>

	<script type="text/javascript">
		var myCenter = new google.maps.LatLng(23.129668993245872, -82.40740561867537);

		var marker = new google.maps.Marker({
			position: myCenter
			,
		});

		window.onload = function () {
			var mapOptions = {
				center: myCenter,
				zoom: 16,
				mapTypeId: google.maps.MapTypeId.ROADMAP,
				panControl: false,
				zoomControl: false,
				scaleControl: false,
			};
			var map = new google.maps.Map(document.getElementById("mapaGoogle"), mapOptions);
			marker.setMap(map);
		};

		//Exportaciones
		$("#renglonesExportacionExp .card").click(function (e) {
			//ir a #listProyectos
			$(".card").removeClass("activeCard");
			$(this).addClass("activeCard");
		});

		//Importaciones
		$("#listProyectos .btn").click(function (e) {
			$("#proyectoSeleccionado").slideDown(400);
		});		 
	</script>

{% block javascripts %}{% endblock %}

</body>

</html>